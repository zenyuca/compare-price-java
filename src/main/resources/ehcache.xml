<?xml version="1.0" encoding="UTF-8"?>
<ehcache>
	<diskStore path="java.io.tmpdir" />
	<defaultCache maxElementsInMemory="10000" eternal="false"
		timeToIdleSeconds="7200" timeToLiveSeconds="7200" overflowToDisk="true" />
	<cache name="loginSession" maxElementsInMemory="10000"
		maxElementsOnDisk="100000" eternal="true" overflowToDisk="true"
		diskPersistent="true" />
	<cache name="token" maxElementsInMemory="10" maxElementsOnDisk="100"
		eternal="false" timeToIdleSeconds="7200" timeToLiveSeconds="7200"
		overflowToDisk="true" diskPersistent="true" />
</ehcache>  