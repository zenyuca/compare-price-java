<?xml version="1.0" encoding="UTF-8"?>
<ehcache>
	<diskStore path="java.io.tmpdir" />
	<defaultCache maxElementsInMemory="500" eternal="false"
		timeToIdleSeconds="300" timeToLiveSeconds="1200" overflowToDisk="true" />
	<cache name="loginSession" maxElementsInMemory="10000"
		maxElementsOnDisk="100000" eternal="false" timeToIdleSeconds="3600"
		timeToLiveSeconds="0" overflowToDisk="true" diskPersistent="true" />
	<cache name="token" maxElementsInMemory="10000"
		maxElementsOnDisk="100000" eternal="false" timeToIdleSeconds="3600"
		timeToLiveSeconds="0" overflowToDisk="true" diskPersistent="true" />
	<cache name="cache" maxElementsInMemory="10000"
		maxElementsOnDisk="100000" eternal="false" timeToIdleSeconds="3600"
		timeToLiveSeconds="0" overflowToDisk="true" diskPersistent="true" />
</ehcache>  